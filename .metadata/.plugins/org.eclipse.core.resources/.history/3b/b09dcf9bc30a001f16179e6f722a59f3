package Assignments;

import java.util.Scanner;

public class NumToBinary {

	public static void main(String[] args) {
		
			Scanner scan = new Scanner(System.in);
			int n = scan.nextInt();
			
			int pv = 1;  
			int answer = 0; 
			int answer1;
			while(n!=0) {
				
	             answer = n%2;
	             n = n/2;
	 			pv = pv * pv;
			}	
	 			answer1 = answer%10;
	 			answer1 /=10;
	 			 System.out.print(answer1);
			
			}
			
			
			
		}

