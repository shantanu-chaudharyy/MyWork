package Arrayy;

public class roughh {

	public static void main(String[] args) {
		
		int arr[] = new int[5];
		
		arr[1] = 1;
		arr[2] = 2;
		arr[0] = 5;
		arr[4] = 4;
		arr[3] = 3;
		
		System.out.println(arr[1]);
		System.out.println(arr[2]);
		System.out.println(arr[3]);
		System.out.println(arr[4]);
		System.out.println(arr[5]);

		
	}

}
